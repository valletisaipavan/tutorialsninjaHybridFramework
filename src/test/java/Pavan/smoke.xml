<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<test thread-count="5" name="loginTest">
		<groups>
			<run>
				<include name="smoke" />
			</run>
		</groups>
		<classes>
			<class name="Pavan.login">
				<methods>
					<include name="loginwithvalidcredentail"/>
					<include name="loginwithinvalidcredentails"/>
				</methods>
			</class>
		</classes>
	</test>
	<test thread-count="5" name="searchTest">
		<groups>
			<run>
				<include name="smoke" />
			</run>
		</groups>
		<classes>
			<class name="Pavan.search">
				<methods>
					<include name="searchvalidproduct"/>
					
				</methods>
			</class>
		</classes>
	</test>
</suite> 
